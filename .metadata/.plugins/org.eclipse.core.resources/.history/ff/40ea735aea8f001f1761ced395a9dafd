import java.util.Locale;

public class Saida_de_Dados {

	public static void main(String[] args) {

		String nome = "maria";
		int idade = 60;
		double renda =  4000.0;
		Locale.setDefault(Locale.US);
		System.out.printf("%s tem %d ano e tem uma rende de R$ %.2f reais%n", nome, idade, renda);
		
		
		
		
		
		

	}

}
