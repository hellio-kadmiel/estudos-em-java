import java.util.Locale;

public class Saida_de_Dados {

	public static void main(String[] args) {

		String nome = "maria";
		int idade = 60;
		double renda =  4000.0;
		Locale.setDefault(Locale.US);
		System.out.printf("%s tem %d ano e tem uma rende de R$ %.2f reais%n", nome, idade, renda);
		
		
		String produte1 = "computer";
		String produte2 = "Office";
		
		
		int age = 20;
		int code =5290;
		char genero = 'F';
		
		double price1 = 2100.0;
		double price2 = 650.50;
		double measure = 53.234567;
		
		
		
		
		System.out.printf("Products: %s , %s %n", produte1, produte2 );
		System.out.printf("a idade eh %d %n" , age);
		System.out.printf("com o codigo:%d %n", code);
		System.out.printf("de genero eh: %c %n", genero);
		System.out.printf("Preco1: %.8f%n", price1);
		System.out.printf("Preco2: %.3f%n ", price2);
		
		
		
		
		
		
		

	}

}
